datasource db {
  provider = "postgresql"
  url      = ""
}

generator client {
  provider = "prisma-client-js"
}

model LiteLLM_ProxyModel {
  model_id       String   @db.Text @id
  model_name     String   @db.Text
  litellm_params Json     @db.JsonB
  model_info     Json?    @db.JsonB
  created_at     DateTime @default(now()) @db.Timestamp(3)
  created_by     String   @db.Text
  updated_at     DateTime @default(now()) @db.Timestamp(3)
  updated_by     String   @db.Text

  @@map("LiteLLM_ProxyModelTable")
}

model LiteLLM_Credentials {
  credential_id     String   @id @db.Text
  credential_name   String   @db.Text
  credential_values Json     @db.JsonB
  credential_info   Json?    @db.JsonB
  created_at        DateTime @default(now()) @db.Timestamp(3)
  created_by        String   @db.Text
  updated_at        DateTime @default(now()) @db.Timestamp(3)
  updated_by        String   @db.Text

  @@map("LiteLLM_CredentialsTable")
}
