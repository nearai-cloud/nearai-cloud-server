datasource db {
  provider = "postgresql"
  url      = env("")
}

generator client {
  provider = "prisma-client-js"
}

model LiteLLM_ProxyModelTable {
  model_id       String   @db.Text         @id
  model_name     String   @db.Text
  litellm_params Json     @db.JsonB
  model_info     Json?    @db.JsonB
  created_at     DateTime @db.Timestamp(3) @default(now())
  created_by     String   @db.Text
  updated_at     DateTime @db.Timestamp(3) @default(now())
  updated_by     String   @db.Text
}

model LiteLLM_CredentialsTable {
  credential_id     String   @db.Text         @id
  credential_name   String   @db.Text
  credential_values Json     @db.JsonB
  credential_info   Json?    @db.JsonB
  created_at        DateTime @db.Timestamp(3) @default(now())
  created_by        String   @db.Text
  updated_at        DateTime @db.Timestamp(3) @default(now())
  updated_by        String   @db.Text
}
